<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <stroke android:color="#4DD9D9D9"
                android:width="1dp"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <item android:left="1dp"
        android:bottom="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <stroke android:color="#4DD9D9D9"
                android:width="1dp"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <item android:left="1dp"
        android:bottom="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <solid android:color="#FFFFFF"/>
            <stroke android:color="#4DD9D9D9"
                android:width="1dp"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>

</layer-list>